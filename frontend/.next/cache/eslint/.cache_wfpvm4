\n  const {\n    register,\n    handleSubmit,\n    formState: { errors },\n  } = useForm();\n\n  const [edit, setEdit] = useState(false);\n  const [hover, setHover] = useState(false);\n  const [value, setValue] = useState(data.name);\n\n  if (edit) {\n    return (\n      <ItemContainerForm onSubmit={() => {}}>\n        <input\n          className=\"input\"\n          value={value}\n          {...register('name')}\n          onChange={(e) => setValue(e.target.value)}\n        />\n        <UtilityWrapper>\n          <UtilityButton variant=\"save\" onClick={() => {}} />\n          <UtilityButton\n            variant=\"close\"\n            onClick={() => {\n              setEdit(false);\n            }}\n          />\n        </UtilityWrapper>\n      </ItemContainerForm>\n    );\n  }\n\n  return (\n    <ItemContainer\n      onClick={() => setHover(true)}\n      onFocus={() => setHover(true)}\n      onBlur={() => setHover(false)}\n    >\n      <span>{data.name}</span>\n\n      {hover && !edit && (\n        <UtilityWrapper>\n          <UtilityButton variant=\"save\" onClick={() => {}} />\n          <UtilityButton\n            variant=\"close\"\n            onClick={() => {\n              onDelete();\n            }}\n          />\n        </UtilityWrapper>\n      )}\n    </ItemContainer>\n  );\n};\n\nconst AddItem = () => {\n  const [add, setAdd] = useState(false);\n\n  return (\n    <Wrapper>\n      {!add ? (\n        <Button\n          type=\"button\"\n          secondary\n          label=\"+ Add ingredient\"\n          onClick={() => setAdd(true)}\n        />\n      ) : (\n        <Form onSubmit={() => {}}>\n          {/* <DropdownList\n            placeholder=\"Search for ingredient\"\n            data={ingredients}\n            dataKey=\"_id\"\n            textField=\"name\"\n            onChange={(val) => setSelectedIngredient(val)}\n          /> */}\n          <UtilityWrapper>\n            <UtilityButton variant=\"save\" onClick={() => {}} />\n            <UtilityButton\n              variant=\"close\"\n              onClick={() => {\n                setAdd(false);\n              }}\n            />\n          </UtilityWrapper>\n        </Form>\n      )}\n    </Wrapper>\n  );\n};\n\nexport const MealDirections = ({ meal }: { meal: any }) => {\n  const [edit, setEdit] = useState(false);\n  const {\n    register,\n    handleSubmit,\n    formState: { errors },\n  } = useForm();\n  const [directions, setDirections] = useState(meal.directions);\n  const [addStep, setAddStep] = useState(false);\n  const [newStep, setNewStep] = useState('');\n\n  return (\n    <div className=\"prose\">\n      <SubContainer>\n        <Header>\n          <h3 className=\"m-2\">Ingredients</h3>\n        </Header>\n        <div className=\"flex flex-col gap-4\">\n          {\n            //   ingredients &&\n            [{ id: '1', name: 'ingredient' }].map((ingredient: any) => (\n              <Item\n                key={`ingredient-${ingredient.id}`}\n                data={ingredient}\n                onDelete={() => {}}\n              />\n            ))\n          }\n\n          <AddItem />\n        </div>\n      </SubContainer>\n\n      <SubContainer>\n        <Header>\n          <h3>Directions</h3>\n        </Header>\n        <>\n          <Steps>\n            {directions &&\n              directions.map((step: any) => (\n                <Step key={step}>\n                  {!edit ? (\n                    <>\n                      <span>{step}</span>\n                      {!edit && (\n                        <>\n                          <UtilityButton variant=\"save\" onClick={() => {}} />\n                          <UtilityButton\n                            variant=\"close\"\n                            onClick={() => {\n                              setEdit(false);\n                            }}\n                          />\n                        </>\n                      )}\n                    </>\n                  ) : (\n                    <DirectionsForm onSubmit={() => {}}>\n                      <input\n                        type=\"textarea\"\n                        value={''}\n                        {...register('directions', {\n                          onChange: (e) => {\n                            setDirections(e.target.value);\n                          },\n                        })}\n                      />\n                      <UtilityWrapper>\n                        <UtilityButton variant=\"save\" onClick={() => {}} />\n                        <UtilityButton\n                          variant=\"close\"\n                          onClick={() => {\n                            setEdit(false);\n                          }}\n                        />\n                      </UtilityWrapper>\n                    </DirectionsForm>\n                  )}\n                </Step>\n              ))}\n          </Steps>\n          {!addStep ? (\n            <Button\n              type=\"button\"\n              secondary\n              onClick={() => setAddStep(true)}\n              label=\"Add Step\"\n            />\n          ) : (\n            <Form>\n              <input\n                type=\"textarea\"\n                placeholder=\"Step directions\"\n                onChange={(e) => setNewStep(e.target.value)}\n              />\n\n              <UtilityWrapper>\n                <UtilityButton variant=\"save\" onClick={() => {}} />\n                <UtilityButton\n                  variant=\"close\"\n                  onClick={() => {\n                    setAddStep(false);\n                  }}\n                />\n              </UtilityWrapper>\n            </Form>\n          )}\n        </>\n      </SubContainer>\n    </div>\n  );\n};\n","/Users/bernardmuller/dev/p/munchies_frontend/components/meal/meal-info/MealInfo.mocks.ts",[],[],"/Users/bernardmuller/dev/p/munchies_frontend/components/meal/meal-info/MealInfo.stories.tsx",[],[],"/Users/bernardmuller/dev/p/munchies_frontend/co